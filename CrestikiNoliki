s = [
    ["-", "-", "-"],
    ["-", "-", "-"],
    ["-", "-", "-"]    
]
for i in range(9):
    if  i % 2 == 0:
        x, y = map(int, input().split())
        x -= 1
        y -= 1
        while x > 2 or y > 2 or s[x][y] != "-":
            x, y = map(int, input().split())
    else: 
        x, y = map(int, input().split())
        while x > 2 or y > 2 or s[x][y] != "-":
            x, y = map(int, input().split())
